{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5099"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7225;http://localhost:5099"
    },
    "http (Release)": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Release"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5099"
    },
    "https (Release)": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Release"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7225;http://localhost:5099"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "ASPNETCORE_URLS": "http://+:8080"
      },
      "publishAllPorts": false,
      "useSSL": false,
      "dockerfileContext": "..",
      "dockerfileFile": "CoreApiBase/Dockerfile.dev",
      "dockerfileRunArguments": "--name coreapi-dev --rm -v coreapi_dev_data:/app/data",
      "dockerfileRunEnvironmentFiles": []
    },
    "Docker (Production)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "ASPNETCORE_URLS": "http://+:8080",
        "DatabaseSettings__ConnectionString": "Data Source=/app/data/appdb.sqlite",
        "DatabaseSettings__AutoMigrate": "true",
        "DatabaseSettings__SeedData": "false",
        "JwtSettings__SecretKey": "ProdDockerSecretKeyForJwtTokenGeneration12345678901234567890",
        "JwtSettings__Issuer": "http://localhost:8080",
        "JwtSettings__Audience": "http://localhost:8080"
      },
      "publishAllPorts": false,
      "useSSL": false,
      "dockerfileContext": "..",
      "dockerfileFile": "CoreApiBase/Dockerfile",
      "dockerfileRunArguments": "--name coreapi-prod --rm -v coreapi_prod_data:/app/data",
      "dockerfileRunEnvironmentFiles": []
    },
    "WSL": {
      "commandName": "WSL2",
      "launchBrowser": true,
      "launchUrl": "https://localhost:7225/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7225;http://localhost:5099"
      },
      "distributionName": ""
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:57238",
      "sslPort": 44300
    }
  }
}